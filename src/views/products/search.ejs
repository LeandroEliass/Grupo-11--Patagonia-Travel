    <h2 class="tituloSeccion"> Busquedas</h1>
        <section class="">
            <ol class="listaHospedajes">
         <% if (product) { %>   
            <% for (i=0;i<product.length; i++) { %> 
                <li>
                  <a class="listHospedajes" href="/products/<%= product[i].id %> "> 
                    <%= product[i].category.category %>  <%= product[i].name %> </li>
